<div>
  <div>
    <div>
        <div>
        <h3> <%= @event.title %> </h3>
        <p> <%= @event.description %> </p>
        <p> <%= @event.date %> </p>
        <p> Event creator: <%= @user.username %> </p>
      </div>
    </div>
  </div>
  <h2>Attendees</h2>
  <% @names.each do |attendee| %>
      <div class="event">
          <h3 class="event-title"> <%= attendee %> </h3>
      </div>
  <% end %>
</div>