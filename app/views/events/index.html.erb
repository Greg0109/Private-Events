<% @events.each do |event| %>
    <div class="event">
        <h1>
            <%= event.title %>
        </h1>
        <p>
            <%= event.description %>
        </p>
        <p>
            <%= event.date %>
        </p>
        <%= link_to "See Event", "/events/show/#{event.id}" %>
    </div>
<% end %>